# API Settings
# Note: SECRET_KEY is defined in the config but not currently used by the application
DEBUG=True

# Database Settings
DATABASE_URL=postgresql://toban_admin:postgres@postgres/tobancv

# Authentication Settings (Supabase)
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_service_role_key
SUPABASE_JWT_SECRET=your_supabase_jwt_secret

# Third-Party API Keys
OPENAI_API_KEY=your_openai_api_key
# Note: Slack credentials are now entered directly in the UI and no longer need to be set as environment variables
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
NOTION_API_KEY=your_notion_api_key

# Feature Flags
ENABLE_SLACK_INTEGRATION=True
ENABLE_GITHUB_INTEGRATION=True
ENABLE_NOTION_INTEGRATION=True

# Logging
LOG_LEVEL=INFO
